pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building the project'
                sleep 5
            }
        }
        stage('Test') {
            steps {
                echo 'Testing the project'
                sleep 3
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
